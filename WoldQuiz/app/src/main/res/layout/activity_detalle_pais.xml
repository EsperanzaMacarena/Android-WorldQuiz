<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    tools:context=".detallePais" >

    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="107dp"
        android:layout_height="108dp"
        android:layout_marginStart="12dp"
        android:layout_marginTop="12dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:srcCompat="@tools:sample/avatars" />

    <TextView
        android:id="@+id/textViewDetallePais"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="44dp"
        android:layout_marginTop="16dp"
        android:text="TextView"
        app:layout_constraintStart_toEndOf="@+id/imageView3"
        app:layout_constraintTop_toTopOf="@+id/imageView3" />

    <TextView
        android:id="@+id/textViewMoneda"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="27dp"
        android:text="TextView"
        app:layout_constraintEnd_toEndOf="@+id/textView8Capital"
        app:layout_constraintStart_toStartOf="@+id/textView8Capital"
        app:layout_constraintTop_toBottomOf="@+id/textView8Capital" />

    <TextView
        android:id="@+id/textView8Capital"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="29dp"
        android:text="TextView"
        app:layout_constraintBottom_toBottomOf="@+id/textViewDetallePais"
        app:layout_constraintStart_toEndOf="@+id/textViewDetallePais"
        app:layout_constraintTop_toTopOf="@+id/textViewDetallePais" />

    <TextView
        android:id="@+id/textViewIdiona"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="27dp"
        android:text="TextView"
        app:layout_constraintEnd_toEndOf="@+id/textViewDetallePais"
        app:layout_constraintStart_toStartOf="@+id/textViewDetallePais"
        app:layout_constraintTop_toBottomOf="@+id/textViewDetallePais" />
</androidx.constraintlayout.widget.ConstraintLayout>